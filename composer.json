{
    "name": "kanellov/functional-php",
    "description": "A library that helps develop in a functional way",
    "type": "library",
    "license": "GPL",
    "keywords": [],
    "support": {},
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Knlv\\Functional\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KnlvTest\\Functional\\": "test"
        }
    },
    "scripts": {
        "test": ["php-cs-fixer fix --dry-run", "phpunit --verbose"],
        "lint": "php-cs-fixer fix"
    }
}
